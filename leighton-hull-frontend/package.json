{
  "name": "leighton-hull-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "docker:dev": "docker compose --env-file ./env/.dev.env  -f ./docker/dev/docker-compose.dev.yml up",
    "start:staging": "docker compose --env-file ./env/.staging.env  -f ./docker/website-staging/docker-compose.staging.yml up --build -d",
    "start:prod": "docker compose --env-file ./env/.prod.env  -f ./docker/website-prod/docker-compose.prod.yml up --build -d"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.10.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "framer-motion": "^12.23.6",
    "hls.js": "^1.6.13",
    "next": "15.4.1",
    "notistack": "^3.0.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "swiper": "^12.0.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
